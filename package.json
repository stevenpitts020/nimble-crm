  "scripts": {
    "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}' --fix --format verbose",
    "start": "DISABLE_ESLINT_PLUGIN=true react-app-rewired start",
    "postinstall": "cp -r src/postinstall/ node_modules/",
    "prebuild": "yarn install; mkdir -p public/pdfjs; cp -f node_modules/pdfjs-dist/legacy/build/pdf.js public/pdfjs; cp -f node_modules/pdfjs-dist/legacy/build/pdf.worker.js public/pdfjs",
    "build": "DISABLE_ESLINT_PLUGIN=true NODE_OPTIONS=--max_old_space_size=4096 react-app-rewired build",
    "test": "TZ=UTC npx jest --bail",
    "test:clear": "react-app-rewired test --clear-cache",
    "eject": "react-scripts eject",
    "analyze": "source-map-explorer build/static/js/main.*",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public",
    "test:ci": "TZ=UTC npx jest --bail --coverage --coverageDirectory=coverage/ --ci --reporters=default --reporters=jest-junit",
    "lint:ci": "npx tslint -c tslint.json 'src/**/*.{ts,tsx}' --format verbose",
    "audit:fix": "npm_config_yes=true npx yarn-audit-fix",
    "generate-canonical-line-items": "DIR='./src/components/Analysis/Statements' && node ${DIR}/LineItemCodeGen.js > ${DIR}/LineItems.tsx"
  },
  "jest": {
    "verbose": true,
    "roots": [
      "<rootDir>/src"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.ts"
    ],
    "preset": "ts-jest",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "babel-jest-amcharts",
      "^.+\\.svg$": "<rootDir>/svgTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\](?!(@amcharts)\\/).+\\.(js|jsx|ts|tsx)$"
    ],
    "coverageReporters": [
      "lcov",
      "text",
      "cobertura"
    ],
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "ie 11",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "ie 11",
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": "14"
  }
}
